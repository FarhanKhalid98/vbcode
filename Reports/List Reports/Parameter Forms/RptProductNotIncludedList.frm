Private Sub CmbTransections_Click()
   Select Case CmbTransections.Text
   Case "Purchase Transections"
      sSql = " Left Outer Join (select * from PurchaseBody where PurchaseDate Between '" & DtpFrom.DateValue & "' and '" & DtpTo.DateValue & "')b on b.ProductID = p.ProductID " & vbCrLf & _
      " Left outer join PurchaseHeader h on h.PurID = b.PurID and h.PurchaseDate = b.PurchaseDate" & vbCrLf & _
      " where b.ProductID is null "
   Case "Sale Transections"
      sSql = " Left Outer Join (select * from SaleBody where BillDate Between '" & DtpFrom.DateValue & "' and '" & DtpTo.DateValue & "')b on b.ProductID = p.ProductID " & vbCrLf & _
      " Left outer join SaleHeader h on h.BillID = b.BillID and h.BillDate = b.BillDate" & vbCrLf & _
      " where b.ProductID is null "
   Case "Opening Stock"
      sSql = " Left outer join OpeningStock op on op.ProductID = p.ProductID " & vbCrLf & _
      " where op.ProductID is null "
   Case "Stock Adjustments"
      sSql = " Left Outer Join (select b.* from StockAdjustmentBody b inner join StockAdjustmentHeader h on h.AdjustmentID = b.AdjustmentID " & vbCrLf & _
      " where AdjustmentDate Between '" & DtpFrom.DateValue & "' and '" & DtpTo.DateValue & "')b on b.ProductID = p.ProductID " & vbCrLf & _
      " where b.ProductID is null "
   End Select
End Sub

Private Sub TxtCode_Change()
