Private Function SetReport() As Boolean
   On Error GoTo ErrorHandler
   Dim RsReport As New ADODB.Recordset
   SetReport = False
   Me.MousePointer = vbHourglass
      VStrSQL = "Select p.*, isnull(Phone1 + ',','') + isnull(Phone2 + ',','') + isnull(Mobile,'') as  ContactNo, Z.ZoneID, ZoneName, SectorName, CustomerType" & vbCrLf _
            + " from Parties p" & vbCrLf _
            + " left outer Join CustomerTypes c on c.CustomerTypeId = p.CustomerTypeID" & vbCrLf _
            + " left outer Join Sectors S on S.sectorId = p.sectorID" & vbCrLf _
            + " left outer join Zones Z on Z.ZoneID = S.ZoneID Where PartyType <> 'V' " & vbCrLf _
            + "" & IIf(Trim(TxtZoneID.Text) = "", "", " And z.ZoneID in ( " & TxtZoneID.Text & ")") & vbCrLf _
            + "" & IIf(Trim(TxtSectorID.Text) = "", "", " And p.SectorID in ( " & TxtSectorID.Text & ")") & vbCrLf _
            + "" & IIf(Trim(TxtCustomerID.Text) = "", "", " And PartyID = " & TxtCustomerID.Text) & vbCrLf _
            + "" & IIf(Trim(TxtCustomerTypeID.Text) = "", "", " And p.CustomerTypeID = " & TxtCustomerTypeID.Text)
   Select Case CmbReportType.Text
      Case "Customer List"
          Set RptReportViewer.Report = New CRptCustomerList
      Case "Customer List (Customer Type Wise)"
          Set RptReportViewer.Report = New CRptCustomerListCustomerTypeWise
      Case "Customer List (Sector Wise)"
          Set RptReportViewer.Report = New CRptCustomerListSectorWise
      Case "Customer List (Zone Wise)"
