   Dim vSQL As String
   'Set RsReport = CN.Execute("EXEC ProdRptCustomerWiseSaleDetail '" & DtpFrom.DateValue & "','" & DtpTo.DateValue & "'," & IIf(Trim(TxtOrganizationID.Text) = "", "Null", "'" & TxtOrganizationID.Text & "'") & "," & IIf(Trim(TxtCustomerID.Text) = "", "Null", "'" & TxtCustomerID.Text & "'") & "," & IIf(Trim(TxtFromBillID.Text) = "", "Null", TxtFromBillID.Text) & "," & IIf(Trim(TxtToBillID.Text) = "", "Null", TxtToBillID.Text) & "," & IIf(RdoBoth.Value = True, "Null", IIf(RdoInv.Value = True, 0, 1)))
   If OptAverage.Value = True Then
      Set RsReport = CN.Execute("EXEC ProdRptCustomerWiseSaleDetail '" & DtpFrom.DateValue & "','" & DtpTo.DateValue & "'," & IIf(Trim(TxtCustomerID.Text) = "", "Null", "'" & TxtCustomerID.Text & "'") & "," & IIf(Trim(TxtFromBillID.Text) = "", "Null", TxtFromBillID.Text) & "," & IIf(Trim(TxtToBillID.Text) = "", "Null", TxtToBillID.Text) & "," & IIf(Trim(TxtOrganizationID.Text) = "", "Null", TxtOrganizationID.Text))
